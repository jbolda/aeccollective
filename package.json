{
  "name": "acecollective",
  "version": "1.0.0",
  "author": "Jacob Bolda <me@jacobbolda.com>",
  "description": "The website for the AEC Collective, an Archiecture Engineering and Construction community.",
  "scripts": {
    "develop": "gatsby develop",
    "dev:hard": "yarn run clean && yarn run develop",
    "build": "yarn clean & yarn jest & gatsby build",
    "serve": "gatsby serve",
    "clean": "rm -rf .cache && rm -rf public",
    "jest": "jest",
    "test": "yarn lint && yarn jest",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "pretty": "prettier --write \"{{src,plugins}/**/*.js,*.js}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbolda/aeccollective.git"
  },
  "license": "SEE LICENSE IN LICENESE FILE",
  "bugs": {
    "url": "https://github.com/jbolda/aeccollective/issues"
  },
  "homepage": "https://github.com/jbolda/aeccollective#readme",
  "dependencies": {
    "gatsby": "2.13.27",
    "gatsby-image": "2.2.6",
    "gatsby-link": "2.2.0",
    "gatsby-plugin-google-tagmanager": "2.1.4",
    "gatsby-plugin-manifest": "2.2.3",
    "gatsby-plugin-sharp": "2.2.8",
    "gatsby-plugin-typography": "2.3.2",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.1.6",
    "gatsby-remark-relative-images": "0.2.2",
    "gatsby-source-filesystem": "2.1.1",
    "gatsby-theme-bulma-core": "2.0.0-alpha.42",
    "gatsby-theme-bulma-layout": "2.0.0-alpha.35",
    "gatsby-transformer-javascript-frontmatter": "2.1.0",
    "gatsby-transformer-remark": "2.6.6",
    "gatsby-transformer-sharp": "2.2.4",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "typography": "0.16.19",
    "typography-theme-lawton": "0.16.19"
  },
  "devDependencies": {
    "jest": "24.8.0",
    "prettier": "1.18.2"
  }
}
